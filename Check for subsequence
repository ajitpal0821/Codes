class Solution{
    public:
    bool isSubSequence(string A, string B) 
    {
       	  int i=0;
        for(int j=0; i<A.size() && j<B.size() ;j++){
            if(A[i] == B[j]){
                i++;
            }
        }
        
        
        if(i == A.length()){
            return 1;
        }
        return 0;
    
		
    }
};
