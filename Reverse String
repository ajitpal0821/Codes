// LeetCode Problem

// https://leetcode.com/problems/reverse-string/

class Solution {
public:
    void reverseString(vector<char>& s) {
        
        int n=s.size();
        for(int i=0;i<s.size()/2;i++){    // check we have operated loop till n/2 only or (left <right)
           swap(s[i],s[n-i-1]);
        }
    
    }
};
