#include <bits/stdc++.h> 
int uniquePaths(int m, int n) {
	// Write your code here.
	int dp[n][m];
	if(n==1 || m==1)
	return 1;
	dp[0][0]=0;
	for(int i=1;i<n;i++){
		dp[i][0]=1;
	}
	for(int i=1;i<m;i++){
		dp[0][i]=1;
	}
	for(int i=1;i<n;i++){
		for(int j=1;j<m;j++){
			dp[i][j]=(dp[i-1][j]+dp[i][j-1]);
		}
	}
	return dp[n-1][m-1];
	
}
